#!/bin/bash

# Generated run script for cs-studio.
prefix=$${CONVERTER_ROOT:-/dls_sw/work/common/CSS/shadow/}

if [ -z "$$1" ]; then
    echo "No OPI specified to launch!"
    exit 1
fi

if [ -n $$2 ]; then
    MACROS="-m $$2"
fi


CSS_RUN_SCRIPT=$$(configure-ioc s -p CSS-beta-gui)

PROJECT=${project}
MODULE=${module}
LAUNCH_OPI=/$$PROJECT/$$MODULE/$$1

LINKS="${links}"

$$CSS_RUN_SCRIPT -o $${LAUNCH_OPI} -s -l "$$LINKS" $$MACROS
